package ds.stack;

public class Stack {
	private int maxSize;
	private long[] stackArray;
	private int top;
	
	public Stack(int size) {
		this.maxSize = size;
		stackArray = new long[this.maxSize];
		this.top=-1;
	}
	
	public void push(long j) {
		this.top++;
		stackArray[this.top]= j;
	}
	public long pop() {
		int oldTop=top;
		this.top--;
		return stackArray[oldTop];
	}
	public long peak() {
		return stackArray[top];
	}
}
